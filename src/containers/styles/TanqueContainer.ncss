
.listContainer {
    padding-top: 66px;
    background: #222;
    min-height: 100vh;
    width: 15%;
    position: fixed;
}

.listContainer li {
    padding: 20px 10px;
    transition: ease-in 0.1s;
}
.listContainer li:hover {
    background-color: #4e544e;
}

.contentContainer {
    padding: 66px 20px;
    background-color: #f1f2f4;
    color: black;
    width: 85%;
    min-height: 100%;
    margin-left: 15%;
}
.my-loader{
    background-color: black;
}


